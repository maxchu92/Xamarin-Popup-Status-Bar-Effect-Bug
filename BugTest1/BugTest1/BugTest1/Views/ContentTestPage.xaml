<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 Title="Content Page"
             x:Class="BugTest1.Views.ContentTestPage">

	<StackLayout VerticalOptions="CenterAndExpand" Padding="16">
		<Button Text="Open Content Page" Clicked="BtnOpenContentPage_Clicked" />
		<Button Text="Open Popup Page" Clicked="BtnOpenPopupPage_Clicked" />
		<Button Text="Back" Clicked="BtnBack_Clicked" />
		<Button Text="Reset Stack" Clicked="BtnResetStack_Clicked" />
	</StackLayout>
</ContentPage>
